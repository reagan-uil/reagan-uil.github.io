    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "svelte": "^5.2.7",
    "svelte-check": "^4.0.9",
    "vite": "^5.4.11"
  },
  "type": "module"
}
